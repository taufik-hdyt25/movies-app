{
  "env": {
    "production": {
      "plugins": [
        "transform-remove-console",
        [
          "module-resolver",
          {
            "root": ["./src"],
            "extensions": [
              ".ios.js",
              ".android.js",
              ".js",
              ".ts",
              ".tsx",
              ".json"
            ],
            "alias": {
              "@components": "./src/components",
              "@constants": "./src/constant",
              "@screens": "./src/screens",
              "@navigation": "./src/navigation",
              "@utils": "./src/utils"
            }
          }
        ]
      ]
    }
  }
}
